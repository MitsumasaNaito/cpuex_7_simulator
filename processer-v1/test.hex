# 簡単なテストプログラム（MIPS機械語）
# 2つの数を足し算して結果を$v0に格納

# 初期化
20080005  # addi $t0, $zero, 5     # $t0 = 5
20090003  # addi $t1, $zero, 3     # $t1 = 3

# 足し算
01095020  # add $t2, $t0, $t1      # $t2 = $t0 + $t1

# 結果を$v0に格納
01401020  # add $v0, $t2, $zero    # $v0 = $t2

# 終了のためのsyscall番号を設定
2002000A  # addi $v0, $zero, 10    # $v0 = 10 (exit syscall)

# 終了
0000000C  # syscall                # システムコール（終了）
